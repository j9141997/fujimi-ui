  "main": "lib/index.js",
  "module": "esm/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "esm",
    "lib"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.build.json",
    "format": "eslint --fix './**/*.ts{,x}'",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint './**/*.ts{,x}'",
    "lint:style": "stylelint './**/*.ts{,x}'",
    "lint:tsc": "tsc --noEmit",
    "build-storybook": "build-storybook --quiet",
    "storybook": "start-storybook -p 6006 -s ./public",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@storybook/addon-a11y": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addons": "^6.4.14",
    "@storybook/cli": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@swc/core": "^1.2.133",
    "@swc/jest": "^0.2.17",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.21",
    "eslint": "^8.7.0",
    "gts": "^3.1.0",
    "jest": "^27.4.7",
    "next": "11.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "use-intersection": "^0.2.1"
  }
